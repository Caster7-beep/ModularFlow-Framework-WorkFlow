{
  "name": "SmartTavern",
  "namespace": "SmartTavern",
  "description": "集成完整SmartTavern工作流的AI对话系统",
  "enabled": true,
  "frontend": {
    "type": "react",
    "path": "frontend_projects/SmartTavern",
    "port": 3000,
    "dev_command": "npm run dev",
    "open_browser": true
  },
  "backend": {
